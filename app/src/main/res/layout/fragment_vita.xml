<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.emrc_transport_tpe.VitalFragment">



    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:background="@color/white"
        android:orientation="vertical" >

        <TableRow
            android:layout_width="fill_parent"
            android:layout_height="wrap_content" >

            <TextView
                android:id="@+id/vita_tv_null"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="8dp"
                android:layout_weight="2.1"
                android:gravity="center"
                android:text="@string/vita_time"
                android:textColor="@color/black"
                android:textSize="15sp"
                tools:ignore="RtlHardcoded" />

            <TextView
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_weight="2"
                android:gravity="center"
                android:text="@string/vita_gcs"
                android:textColor="@color/black"
                android:textSize="15sp" />


            <TextView
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_weight="2.05"
                android:gravity="center"
                android:text="@string/vita_bp"
                android:textColor="@color/black"
                android:textSize="15sp" />

            <TextView
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_weight="2.1"
                android:gravity="center"
                android:text="@string/vita_pulse"
                android:textColor="@color/black"
                android:textSize="15sp"
                tools:ignore="RtlHardcoded" />

            <TextView
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_weight="2.1"
                android:gravity="center"
                android:text="@string/vita_respiration"
                android:textColor="@color/black"
                android:textSize="15sp" />

            <TextView
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1.3"
                android:gravity="center"
                android:text="@string/vita_evm"
                android:textColor="@color/black"
                android:textSize="15sp" />
        </TableRow>

        <TextView
            android:layout_width="fill_parent"
            android:layout_height="2dp"
            android:drawableTop="@drawable/gray_baseline2" />

        <TableRow
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="3dp"
            android:layout_marginRight="3dp"
            tools:ignore="RtlHardcoded" >

            <TextView
                android:id="@+id/vita_item_0_time"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_weight="2.1"
                android:gravity="center"
                android:hint="@string/vita_time_hint"
                android:singleLine="true"
                android:textColor="@color/black"
                android:textSize="14sp"
                tools:ignore="RtlHardcoded" />


            <Button
                android:id="@+id/vita_item_0_gcs"
                android:layout_width="fill_parent"
                android:layout_height="25dp"
                android:layout_marginRight="3dp"
                android:layout_weight="2"
                android:background="@drawable/item_background_square"
                android:text="@string/s_List_8"
                android:textColor="@color/black"
                tools:ignore="RtlHardcoded,HardcodedText" />

            <EditText
                android:id="@+id/vita_item_0_bp"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_weight="2.05"
                android:gravity="center"
                android:hint="@string/vita_bp_hint"
                android:inputType="number"
                android:maxLength="6"
                android:nextFocusDown="@+id/vita_item_0_pulse"
                android:textColor="@color/black"
                android:textSize="14sp" />

            <EditText
                android:id="@+id/vita_item_0_pulse"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_weight="2.1"
                android:gravity="center"
                android:hint="@string/vita_pulse_hint"
                android:inputType="number"
                android:maxLength="3"
                android:nextFocusDown="@+id/vita_item_0_respiration"
                android:textColor="@color/black"
                android:textSize="14sp"
                tools:ignore="Deprecated" />

            <EditText
                android:id="@+id/vita_item_0_respiration"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_weight="2.1"
                android:gravity="center"
                android:hint="@string/vita_respiration_hint"
                android:imeOptions="actionDone"
                android:inputType="number"
                android:maxLength="3"
                android:textColor="@color/black"
                android:textSize="14sp" />

            <Button
                android:id="@+id/vita_item_0_e"
                android:layout_width="fill_parent"
                android:layout_height="25dp"
                android:layout_marginRight="3dp"
                android:layout_weight="2"
                android:background="@drawable/item_background_square"
                android:text="@string/gcs_e"
                android:textColor="@color/black"
                tools:ignore="RtlHardcoded,HardcodedText" />

            <Button
                android:id="@+id/vita_item_0_v"
                android:layout_width="fill_parent"
                android:layout_height="25dp"
                android:layout_marginRight="3dp"
                android:layout_weight="2"
                android:background="@drawable/item_background_square"
                android:text="@string/gcs_v"
                android:textColor="@color/black"
                tools:ignore="RtlHardcoded,HardcodedText" />

            <Button
                android:id="@+id/vita_item_0_m"
                android:layout_width="fill_parent"
                android:layout_height="25dp"
                android:layout_marginRight="3dp"
                android:layout_weight="2"
                android:background="@drawable/item_background_square"
                android:text="@string/gcs_m"
                android:textColor="@color/black"
                tools:ignore="RtlHardcoded,HardcodedText" />
        </TableRow>

        <TextView
            android:layout_width="fill_parent"
            android:layout_height="2dp"
            android:drawableTop="@drawable/gray_baseline2" />

        <TableRow
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="3dp"
            android:layout_marginRight="3dp"
            tools:ignore="RtlHardcoded" >

            <TextView
                android:id="@+id/vita_item_1_time"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_weight="2.1"
                android:gravity="center"
                android:hint="@string/vita_time_hint"
                android:singleLine="true"
                android:textColor="@color/black"
                android:textSize="14sp"
                tools:ignore="RtlHardcoded" />

            <Button
                android:id="@+id/vita_item_1_gcs"
                android:layout_width="fill_parent"
                android:layout_height="25dp"
                android:layout_marginRight="3dp"
                android:layout_weight="2"
                android:background="@drawable/item_background_square"
                android:text="@string/s_List_8"
                android:textColor="@color/black"
                tools:ignore="RtlHardcoded,HardcodedText" />

            <EditText
                android:id="@+id/vita_item_1_bp"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_weight="2.05"
                android:gravity="center"
                android:hint="@string/vita_bp_hint"
                android:inputType="number"
                android:maxLength="6"
                android:nextFocusDown="@+id/vita_item_1_pulse"
                android:textColor="@color/black"
                android:textSize="14sp" />

            <EditText
                android:id="@+id/vita_item_1_pulse"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_weight="2.1"
                android:gravity="center"
                android:hint="@string/vita_pulse_hint"
                android:inputType="number"
                android:maxLength="3"
                android:nextFocusDown="@+id/vita_item_1_respiration"
                android:textColor="@color/black"
                android:textSize="14sp" />

            <EditText
                android:id="@+id/vita_item_1_respiration"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_weight="2.1"
                android:gravity="center"
                android:hint="@string/vita_respiration_hint"
                android:imeOptions="actionDone"
                android:inputType="number"
                android:maxLength="3"
                android:textColor="@color/black"
                android:textSize="14sp" />

            <Button
                android:id="@+id/vita_item_1_e"
                android:layout_width="fill_parent"
                android:layout_height="25dp"
                android:layout_marginRight="3dp"
                android:layout_weight="2"
                android:background="@drawable/item_background_square"
                android:text="@string/gcs_e"
                android:textColor="@color/black"
                tools:ignore="RtlHardcoded,HardcodedText" />

            <Button
                android:id="@+id/vita_item_1_v"
                android:layout_width="fill_parent"
                android:layout_height="25dp"
                android:layout_marginRight="3dp"
                android:layout_weight="2"
                android:background="@drawable/item_background_square"
                android:text="@string/gcs_v"
                android:textColor="@color/black"
                tools:ignore="RtlHardcoded,HardcodedText" />

            <Button
                android:id="@+id/vita_item_1_m"
                android:layout_width="fill_parent"
                android:layout_height="25dp"
                android:layout_marginRight="3dp"
                android:layout_weight="2"
                android:background="@drawable/item_background_square"
                android:text="@string/gcs_m"
                android:textColor="@color/black"
                tools:ignore="RtlHardcoded,HardcodedText" />
        </TableRow>

        <TextView
            android:layout_width="fill_parent"
            android:layout_height="2dp"
            android:drawableTop="@drawable/gray_baseline2" />

        <TableRow
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="3dp"
            android:layout_marginRight="3dp"
            tools:ignore="RtlHardcoded" >

            <TextView
                android:id="@+id/vita_item_2_time"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_weight="2.1"
                android:gravity="center"
                android:hint="@string/vita_time_hint"
                android:singleLine="true"
                android:textColor="@color/black"
                android:textSize="14sp"
                tools:ignore="RtlHardcoded" />

            <Button
                android:id="@+id/vita_item_2_gcs"
                android:layout_width="fill_parent"
                android:layout_height="25dp"
                android:layout_marginRight="3dp"
                android:layout_weight="2"
                android:background="@drawable/item_background_square"
                android:text="@string/s_List_8"
                android:textColor="@color/black"
                tools:ignore="RtlHardcoded,HardcodedText" />

            <EditText
                android:id="@+id/vita_item_2_bp"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_weight="2.05"
                android:gravity="center"
                android:hint="@string/vita_bp_hint"
                android:inputType="number"
                android:maxLength="6"
                android:nextFocusDown="@+id/vita_item_2_pulse"
                android:textColor="@color/black"
                android:textSize="14sp" />

            <EditText
                android:id="@+id/vita_item_2_pulse"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_weight="2.1"
                android:gravity="center"
                android:hint="@string/vita_pulse_hint"
                android:inputType="number"
                android:maxLength="3"
                android:nextFocusDown="@+id/vita_item_2_respiration"
                android:textColor="@color/black"
                android:textSize="14sp" />

            <EditText
                android:id="@+id/vita_item_2_respiration"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_weight="2.1"
                android:gravity="center"
                android:hint="@string/vita_respiration_hint"
                android:imeOptions="actionDone"
                android:inputType="number"
                android:maxLength="3"
                android:textColor="@color/black"
                android:textSize="14sp" />

            <Button
                android:id="@+id/vita_item_2_e"
                android:layout_width="fill_parent"
                android:layout_height="25dp"
                android:layout_marginRight="3dp"
                android:layout_weight="2"
                android:background="@drawable/item_background_square"
                android:text="@string/gcs_e"
                android:textColor="@color/black"
                tools:ignore="RtlHardcoded,HardcodedText" />

            <Button
                android:id="@+id/vita_item_2_v"
                android:layout_width="fill_parent"
                android:layout_height="25dp"
                android:layout_marginRight="3dp"
                android:layout_weight="2"
                android:background="@drawable/item_background_square"
                android:text="@string/gcs_v"
                android:textColor="@color/black"
                tools:ignore="RtlHardcoded,HardcodedText" />

            <Button
                android:id="@+id/vita_item_2_m"
                android:layout_width="fill_parent"
                android:layout_height="25dp"
                android:layout_marginRight="3dp"
                android:layout_weight="2"
                android:background="@drawable/item_background_square"
                android:text="@string/gcs_m"
                android:textColor="@color/black"
                tools:ignore="RtlHardcoded,HardcodedText" />
        </TableRow>

    </LinearLayout>

</FrameLayout>

<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    android:orientation="horizontal" >



    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:orientation="vertical"
        tools:ignore="UseCompoundDrawables" >

        <ImageView
            android:layout_width="768dp"
            android:layout_height="952dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:scaleType="fitXY"
            android:src="@drawable/ic_page6"
            tools:ignore="ContentDescription" />

    </LinearLayout>

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:orientation="vertical"
        tools:ignore="UseCompoundDrawables" >

            <TextView
                android:id="@+id/TextView_emrgn"
                android:layout_width="339dp"
                android:layout_height="445dp"
                android:layout_marginLeft="71dp"
                android:layout_marginTop="58dp"
                android:orientation="vertical"
                android:text="@string/s_ts_65"
                android:textSize="25sp"
                android:textColor="@color/black"
                android:background="@color/white"/>


    </LinearLayout>


    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:orientation="vertical"
        tools:ignore="UseCompoundDrawables" >


        <LinearLayout
            android:id="@+id/LinearLayout_emrgn"
            android:layout_width="339dp"
            android:layout_height="445dp"
            android:layout_marginLeft="71dp"
            android:layout_marginTop="58dp"
            android:orientation="vertical"
            android:background="@color/white"/>

    </LinearLayout>




    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:orientation="vertical"
        tools:ignore="UseCompoundDrawables" >

        <ImageView
            android:id="@+id/body_front"
            android:layout_width="266dp"
            android:layout_height="358dp"
            android:layout_marginLeft="405dp"
            android:layout_marginTop="0dp"
            android:src="@drawable/inju_front"
            android:orientation="horizontal" >
        </ImageView>

    </LinearLayout>

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:orientation="vertical"
        tools:ignore="UseCompoundDrawables" >

        <ImageView
            android:id="@+id/body_back"
            android:layout_width="266dp"
            android:layout_height="358dp"
            android:layout_marginLeft="480dp"
            android:layout_marginTop="200dp"
            android:src="@drawable/inju_back"
            android:orientation="horizontal" >
        </ImageView>

    </LinearLayout>

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:orientation="vertical"
        tools:ignore="UseCompoundDrawables" >

        <ImageView
            android:id="@+id/body_front_view"
            android:layout_width="266dp"
            android:layout_height="358dp"
            android:layout_marginLeft="405dp"
            android:layout_marginTop="0dp"
            android:orientation="horizontal" >
        </ImageView>

    </LinearLayout>

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:orientation="vertical"
        tools:ignore="UseCompoundDrawables" >

        <ImageView
            android:id="@+id/body_back_view"
            android:layout_width="266dp"
            android:layout_height="358dp"
            android:layout_marginLeft="480dp"
            android:layout_marginTop="200dp"
            android:orientation="horizontal" >
        </ImageView>

    </LinearLayout>

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:orientation="vertical"
        tools:ignore="UseCompoundDrawables" >

        <LinearLayout
            android:id="@+id/LinearLayout_inju"
            android:layout_width="322dp"
            android:layout_height="447dp"
            android:layout_marginLeft="413dp"
            android:layout_marginTop="57dp"
            android:background="@color/white"
            android:orientation="horizontal" >
        </LinearLayout>

    </LinearLayout>


    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:orientation="vertical"
        tools:ignore="UseCompoundDrawables" >


        <LinearLayout
            android:id="@+id/LinearLayout_vita"
            android:layout_width="664dp"
            android:layout_height="144dp"
            android:layout_marginLeft="71dp"
            android:layout_marginTop="608dp"
            android:orientation="vertical"
            android:background="@color/white"/>

    </LinearLayout>

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:orientation="vertical"
        tools:ignore="UseCompoundDrawables" >


            android:gravity="top"
            android:textSize="20sp"
            android:textColor="@color/black"
            android:background="@color/white"/>

    </LinearLayout>
    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:orientation="vertical"
        tools:ignore="UseCompoundDrawables" >


        <ImageView
            android:id="@+id/sign1"
            android:layout_width="60dp"
            android:layout_height="40dp"
            android:layout_marginLeft="305dp"
            android:layout_marginTop="565dp"
            android:maxLength="8"
            android:background="@color/white"
            android:textSize="22sp"/>

    </LinearLayout>



    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:orientation="horizontal"
        tools:ignore="UseCompoundDrawables" >

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        tools:ignore="UseCompoundDrawables" >

        <ImageView
            android:id="@+id/sign2"
            android:layout_width="75dp"
            android:layout_height="30dp"
            android:layout_marginLeft="95dp"
            android:layout_marginTop="800dp" />

        <ImageView
            android:id="@+id/sign3"
            android:layout_width="75dp"
            android:layout_height="30dp"
            android:layout_marginLeft="95dp"
            android:layout_marginTop="2dp" />

        <ImageView
            android:id="@+id/sign4"
            android:layout_width="75dp"
            android:layout_height="30dp"
            android:layout_marginLeft="95dp"
            android:layout_marginTop="2dp" />

    </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            tools:ignore="UseCompoundDrawables" >

            <EditText
                android:id="@+id/sign00_num"
                android:layout_width="45dp"
                android:layout_height="30dp"
                android:layout_marginLeft="55dp"
                android:layout_marginTop="800dp"
                android:background="@color/white"
                android:inputType="number"
                android:maxLength="4" />

            <EditText
                android:id="@+id/sign01_num"
                android:layout_width="45dp"
                android:layout_height="30dp"
                android:layout_marginLeft="55dp"
                android:layout_marginTop="2dp"
                android:background="@color/white"
                android:inputType="number"
                android:maxLength="4" />

            <EditText
                android:id="@+id/sign02_num"
                android:layout_width="45dp"
                android:layout_height="30dp"
                android:layout_marginLeft="55dp"
                android:layout_marginTop="2dp"
                android:background="@color/white"
                android:inputType="number"
                android:maxLength="4" />

        </LinearLayout>



        <ImageView
            android:id="@+id/sign5"
            android:layout_width="148dp"
            android:layout_height="100dp"
            android:layout_marginLeft="7dp"
            android:layout_marginTop="796dp"
            android:background="@color/white"
            tools:ignore="RtlHardcoded,HardcodedText" />

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:layout_marginLeft="6dp"
        android:layout_marginTop="795dp"
        tools:ignore="UseCompoundDrawables" >

        <CheckBox
            android:id="@+id/ambu_pass"
            android:layout_width="148dp"
            android:layout_height="30dp"
            android:text="已告知傷病人或家屬病情需要送醫診治及未送醫之後果"
            android:textSize="9sp"/>

        <ImageView
            android:id="@+id/sign6"
            android:layout_width="148dp"
            android:layout_height="70dp"
            android:background="@drawable/sign_b2"
            tools:ignore="RtlHardcoded,HardcodedText" />


    </LinearLayout>

        <ImageView
            android:id="@+id/sign7"
            android:layout_width="150dp"
            android:layout_height="100dp"
            android:layout_marginLeft="4dp"
            android:layout_marginTop="796dp"
            android:background="@color/white"
            tools:ignore="RtlHardcoded,HardcodedText" />

    </LinearLayout>

    <ImageView
        android:id="@+id/main_6_ok"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentRight="true"
        android:layout_marginBottom="5dp"
        android:layout_marginRight="5dp"
        android:src="@drawable/fs_check_1"
        tools:ignore="ContentDescription,RtlHardcoded" />

    <ImageView
        android:id="@+id/main_6_up"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentRight="true"
        android:layout_marginBottom="5dp"
        android:layout_marginRight="75dp"
        android:src="@drawable/fs_print"
        tools:ignore="ContentDescription,RtlHardcoded" />

</RelativeLayout>